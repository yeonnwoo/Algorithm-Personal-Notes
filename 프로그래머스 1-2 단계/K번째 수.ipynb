    "import copy\n",
    "def solution(array, commands):\n",
    "    answer = []      \n",
    "    for c in commands:\n",
    "        i=c[0]\n",
    "        j=c[1]\n",
    "        k=c[2]\n",
    "        List=[array[i] for i in range(i-1,j)]\n",
    "        List.sort()\n",
    "        \n",
    "        answer.append(List[k-1])\n",
    "    return answer"
   ]
